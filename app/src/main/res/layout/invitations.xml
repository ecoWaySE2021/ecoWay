<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EBFFEB"
    android:backgroundTint="#EBFFEB" >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="411dp"
        android:layout_height="52dp"
        android:background="#EBFFEB"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <ImageButton
            android:id="@+id/imageButton15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#EBFFEB"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.008"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.555"
            app:srcCompat="@drawable/dropdown1" />

        <ImageView
            android:id="@+id/imageView30"
            android:layout_width="112dp"
            android:layout_height="42dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.408"
            app:layout_constraintStart_toEndOf="@+id/imageButton15"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.529"
            app:srcCompat="@drawable/ecoway" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="408dp"
        android:layout_height="685dp"
        android:layout_marginTop="10dp"
        android:background="#EBFFEB"
        android:backgroundTint="#EBFFEB"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2"
        app:layout_constraintVertical_bias="0.875">

        <TextView
            android:id="@+id/textView117"
            android:layout_width="151dp"
            android:layout_height="34dp"
            android:shadowColor="#2E2929"
            android:text="         Invitations"
            android:textColor="#000000"
            android:textSize="16sp"
            android:textStyle="bold|italic"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.445"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.024" />

        <Button
            android:id="@+id/makeInv"
            android:layout_width="408dp"
            android:layout_height="47dp"
            android:backgroundTint="#C1D6C1"
            android:backgroundTintMode="screen"
            android:hapticFeedbackEnabled="true"
            android:text="ΔΗΜΙΟΥΡΓΙΑ ΠΡΟΣΚΛΗΣΗΣ"
            android:textAlignment="textStart"
            android:textColor="#000000"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.063"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView117"
            app:layout_constraintVertical_bias="0.076" />

        <Button
            android:id="@+id/activeInv"
            android:layout_width="403dp"
            android:layout_height="51dp"
            android:layout_marginTop="26dp"
            android:backgroundTint="#C1D6C1"
            android:text="ΕΝΕΡΓΕΣ ΠΡΟΣΚΛΗΣΕΙΣ"
            android:textAlignment="textStart"
            android:textColor="#020202"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/makeInv"
            app:layout_constraintVertical_bias="0.0" />

        <Button
            android:id="@+id/mineInv"
            android:layout_width="406dp"
            android:layout_height="52dp"
            android:backgroundTint="#C1D6C1"
            android:text="ΟΙ ΠΡΟΣΚΛΗΣΕΙΣ ΜΟΥ"
            android:textAlignment="textStart"
            android:textColor="#000000"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/activeInv"
            app:layout_constraintVertical_bias="0.043" />

        <Button
            android:id="@+id/accInv"
            android:layout_width="406dp"
            android:layout_height="53dp"
            android:backgroundTint="#C1D6C1"
            android:text="ΑΠΟΔΕΧΟΥΜΕΝΕΣ ΠΡΟΣΚΛΗΣΕΙΣ"
            android:textAlignment="textStart"
            android:textColor="#000000"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mineInv"
            app:layout_constraintVertical_bias="0.081" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>